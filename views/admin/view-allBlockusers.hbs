<section>
  <div class="container mt-5">


    <table class="table mt-3" id="DataTable">
      <thead class="thead-dark">
        <tr>
          <th scope="col">No</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Phone Number</th>

        </tr>
      </thead>
      <tbody>
        {{#each allBlockUsers}}
        <tr>
          <td>{{inc @index}}</td>
          <td>{{this.name}}</td>
          <td>{{this.email}}</td>
          <td>{{this.phone}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</section>